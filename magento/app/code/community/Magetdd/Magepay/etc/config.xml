            </magetdd_magepay_resource>
        </models>
        <resources>
          <magetdd_magepay_setup>
              <setup>
                  <module>Magetdd_Magepay</module>
              </setup>
          </magetdd_magepay_setup>
        </resources>
    </global>
</config>
